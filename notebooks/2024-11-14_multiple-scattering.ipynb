   "source": [
    "import sympy as sym"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "0dee6402-4019-40e8-8d7c-bc6a781dbae9",
   "metadata": {},
   "outputs": [],
   "source": [
    "x, beta, p, z, x0 = sym.symbols(\"x beta p z X0\")\n",
    "\n",
    "theta0 = 13.6 / (beta * p) * z * sym.sqrt(x / x0) * (1 + 0.038 * sym.ln(x * z**2 / (x0 * beta**2)))\n",
    "\n",
    "theta0"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "8ba0ebe6-3416-4742-9ffc-f0878cb754c6",
   "metadata": {},
   "outputs": [],
   "source": [
    "d = sym.symbols(\"d\")\n",
    "\n",
    "sym.integrate(theta0, (x, 0, d)).simplify()"
   ]
  }
 ],
